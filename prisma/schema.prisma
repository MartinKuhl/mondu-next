generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider  = "postgresql"
    // Uses connection pooling
    url       = env("POSTGRES_PRISMA_URL")
    directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Webhooks {
    id          Int      @id @default(autoincrement())
    createdAt   DateTime @default(now())
    topic       String
    order_uuid  String
    order_state String
    event_time  DateTime
    payload     String
}
